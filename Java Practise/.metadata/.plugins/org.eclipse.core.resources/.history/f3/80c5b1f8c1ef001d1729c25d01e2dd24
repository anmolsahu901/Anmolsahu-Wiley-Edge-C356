package Greedy;

import java.util.ArrayList;
import java.util.Scanner;

public class A1 {

	public static void main(String[] args) {
		Scanner obj = new Scanner(System.in);
		int n = obj.nextInt();
		
		ArrayList<Integer> list = new ArrayList<>();
			
		while(n!=0) {
			if(n>=5) {
				int temp = n/5;
				for(int i=0;i<temp;i++)
					list.add(5);
				n = n%5;
			}
			if(n>=2) {
				int temp = n/2;
				for(int i=0;i<temp;i++)
					list.add(2);
				n = n%2;
			}
			if(n>=1) {
				for(int i=0;i<n;i++)
					list.add(1);
			}
			
			
		}
		
		System.out.println(list);
	}

}
