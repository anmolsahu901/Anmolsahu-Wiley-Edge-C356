package Lambda;

public class A2LambdaClass {

	static void printThing(Printable thing) {
		thing.print();
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A1Product myProd = new A1Product();
//		myProd.print();
		
//		printThing(myProd);
		printThing(()->{
			System.out.println("Lambda Expression");
		});
		
	}

}
