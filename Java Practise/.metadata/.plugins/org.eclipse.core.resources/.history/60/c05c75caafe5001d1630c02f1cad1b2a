package generics;

public class A1GenericsClass {

	public static void main(String[] args) {
		Printer<Double> printer = new Printer<Double>(27.4);
		Printer<String> printer2 = new Printer("27.4");
		Printer<Integer> printer3 = new Printer<Integer>(900);

		
		printer.print();
		printer2.print();
		printer3.print();
	}

}
