package JavaThreads;

class A extends Thread{
	void show() {
		System.out.println("Hii");
	}
}

class B extends Thread{
	void show() {
		System.out.println("Hello");
	}
}

public class A1ThreadBasic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		A a = new A();
		B b = new B();
		a.show();
		b.show();
		
	}

}
